<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.MenuFragment">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#FAF8F8"
            >
            <com.airbnb.lottie.LottieAnimationView
                android:layout_width="200dp"
                android:layout_height="200dp"
                app:lottie_rawRes="@raw/loading"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                android:id="@+id/loadinglottie"
                app:lottie_autoPlay="true"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="50dp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="20dp"
                android:text=" Feedback "
                android:textColor="#525151"
                android:textSize="22sp"
                android:id="@+id/settings_logout"
                />
            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:id="@+id/settings_vv1"
                app:layout_constraintTop_toBottomOf="@+id/settings_logout"
                android:background="#AAAAAA"
                app:layout_constraintLeft_toLeftOf="@+id/settings_logout"
                app:layout_constraintRight_toRightOf="parent"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_marginTop="10dp"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/settings_vv1"
                android:layout_marginTop="10dp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="20dp"
                android:text=" Developer Contact"
                android:textColor="#525151"
                android:textSize="22sp"
                android:id="@+id/settings_help"
                />
            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:id="@+id/settings_vv2"
                app:layout_constraintTop_toBottomOf="@+id/settings_help"
                android:background="#AAAAAA"
                app:layout_constraintLeft_toLeftOf="@+id/settings_help"
                app:layout_constraintRight_toRightOf="parent"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_marginTop="10dp"
                />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/settings_vv2"
                android:layout_marginTop="10dp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="20dp"
                android:text=" Logout "
                android:textColor="#525151"
                android:textSize="22sp"
                android:id="@+id/settings_logout1"
                />
            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:id="@+id/settings_vv3"
                app:layout_constraintTop_toBottomOf="@+id/settings_logout1"
                android:background="#AAAAAA"
                app:layout_constraintLeft_toLeftOf="@+id/settings_logout1"
                app:layout_constraintRight_toRightOf="parent"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_marginTop="10dp"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/settings_vv3"
                android:layout_marginTop="10dp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="20dp"
                android:text=" Restore "
                android:textColor="#525151"
                android:textSize="22sp"
                android:id="@+id/settings_restore"
                />
            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:id="@+id/settings_vv4"
                app:layout_constraintTop_toBottomOf="@+id/settings_restore"
                android:background="#AAAAAA"
                app:layout_constraintLeft_toLeftOf="@+id/settings_restore"
                app:layout_constraintRight_toRightOf="parent"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_marginTop="10dp"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </FrameLayout>
</layout>